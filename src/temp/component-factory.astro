import GraphQLConnectedDemo from '../../src/components/graphql/GraphQL-ConnectedDemo.astro'
import GraphQLIntegratedDemo from '../../src/components/graphql/GraphQL-IntegratedDemo.astro'
import GraphQLLayout from '../../src/components/graphql/GraphQL-Layout.astro'
import StyleguideComponentParams from '../../src/components/styleguide/Styleguide-ComponentParams.astro'
import StyleguideCustomRouteType from '../../src/components/styleguide/Styleguide-CustomRouteType.astro'
import StyleguideEditFrame from '../../src/components/styleguide/Styleguide-EditFrame.astro'
import StyleguideLayoutReuse from '../../src/components/styleguide/Styleguide-Layout-Reuse.astro'
import StyleguideLayoutTabsTab from '../../src/components/styleguide/Styleguide-Layout-Tabs-Tab.astro'
import StyleguideLayoutTabs from '../../src/components/styleguide/Styleguide-Layout-Tabs.astro'
import StyleguideLayout from '../../src/components/styleguide/Styleguide-Layout.astro'
import StyleguideMultilingual from '../../src/components/styleguide/Styleguide-Multilingual.astro'
import StyleguideReact from '../../src/components/styleguide/Styleguide-React.astro'
import StyleguideRouteFields from '../../src/components/styleguide/Styleguide-RouteFields.astro'
import StyleguideSection from '../../src/components/styleguide/Styleguide-Section.astro'
import StyleguideSitecoreContext from '../../src/components/styleguide/Styleguide-SitecoreContext.astro'
import StyleguideSpecimen from '../../src/components/styleguide/Styleguide-Specimen.astro'
import StyleguideTracking from '../../src/components/styleguide/Styleguide-Tracking.astro'

export interface Props  {
  name: string;
  route: any;
}
---

<>
{Astro.props.name === 'ContentBlock' && <ContentBlock route={Astro.props.route}/>}
{Astro.props.name === 'React-ContentBlock' && <ReactContentBlock route={Astro.props.route}/>}
{Astro.props.name === 'React-GraphQL-Layout' && <ReactGraphQLLayout route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-AstroImage' && <StyleguideFieldUsageAstroImage route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-Checkbox' && <StyleguideFieldUsageCheckbox route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-ContentList' && <StyleguideFieldUsageContentList route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-Custom' && <StyleguideFieldUsageCustom route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-Date' && <StyleguideFieldUsageDate route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-File' && <StyleguideFieldUsageFile route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-Image' && <StyleguideFieldUsageImage route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-ItemLink' && <StyleguideFieldUsageItemLink route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-Link' && <StyleguideFieldUsageLink route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-Number' && <StyleguideFieldUsageNumber route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-RichText' && <StyleguideFieldUsageRichText route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-FieldUsage-Text' && <StyleguideFieldUsageText route={Astro.props.route}/>}
{Astro.props.name === 'GraphQL-ConnectedDemo' && <GraphQLConnectedDemo route={Astro.props.route}/>}
{Astro.props.name === 'GraphQL-IntegratedDemo' && <GraphQLIntegratedDemo route={Astro.props.route}/>}
{Astro.props.name === 'GraphQL-Layout' && <GraphQLLayout route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-ComponentParams' && <StyleguideComponentParams route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-CustomRouteType' && <StyleguideCustomRouteType route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-EditFrame' && <StyleguideEditFrame route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-Layout-Reuse' && <StyleguideLayoutReuse route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-Layout-Tabs-Tab' && <StyleguideLayoutTabsTab route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-Layout-Tabs' && <StyleguideLayoutTabs route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-Layout' && <StyleguideLayout route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-Multilingual' && <StyleguideMultilingual route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-React' && <StyleguideReact route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-RouteFields' && <StyleguideRouteFields route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-Section' && <StyleguideSection route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-SitecoreContext' && <StyleguideSitecoreContext route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-Specimen' && <StyleguideSpecimen route={Astro.props.route}/>}
{Astro.props.name === 'Styleguide-Tracking' && <StyleguideTracking route={Astro.props.route}/>}
  {Astro.props.name === undefined && Astro.props.route.name === 'code' && <code {...Astro.props.route.attributes} set:html={Astro.props.route.contents}></code>}
</>
