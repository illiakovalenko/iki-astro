{
  "name": "iki-astro",
  "version": "0.0.1",
  "config": {
    "appName": "AstroApp",
    "rootPlaceholders": [
      "AstroApp-jss-main"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "language": "en"
  },
  "scripts": {
    "bootstrap": "ts-node --project tsconfig.scripts.json scripts/bootstrap.ts",
    "dev": "npm run bootstrap && astro dev --port 3000",
    "start": "astro dev --verbose",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.3",
    "@astrojs/node": "^7.0.3",
    "@astrojs/react": "^3.0.9",
