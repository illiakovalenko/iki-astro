    "@sitecore-jss/sitecore-jss": "^21.7.0-canary.38",
    "@sitecore-jss/sitecore-jss-nextjs": "^21.7.0-canary.38",
    "@sitecore-jss/sitecore-jss-react": "^21.7.0-canary.38",
    "astro": "^4.0.7",
    "bootstrap": "^5.2.3",
    "constant-case": "^3.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@sitecore-jss/sitecore-jss-cli": "^21.7.0-canary.38",
    "@sitecore-jss/sitecore-jss-dev-tools": "^21.7.0-canary.38",
    "@types/constant-case": "^1.1.2",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
